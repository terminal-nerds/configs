{
	"$schema": "https://json.schemastore.org/package",
	"type": "module",
	"name": "@terminal-nerds/browserslist-config",
	"version": "0.3.1",
	"description": "Terminal Nerds extendable configuration for browserslist.",
	"keywords": [
		"config",
		"tool",
		"browserslist"
	],
	"license": "MIT",
	"homepage": "https://github.com/terminal-nerds/configs/tree/main/packages/browserslist",
	"repository": {
		"type": "git",
		"url": "https://github.com/terminal-nerds/configs.git",
		"directory": "packages/browserslist"
	},
	"bugs": "https://github.com/terminal-nerds/configs/issues",
	"engines": {
		"node": ">=18"
	},
	"exports": {
		"./browsers": {
			"types": "./dist/browsers.d.ts",
			"import": "./dist/browsers.js",
			"require": "./dist/browsers.cjs"
		},
		"./node": {
			"types": "./dist/node.d.ts",
			"import": "./dist/browsers.js",
			"require": "./dist/node.cjs"
		}
	},
	"files": [
		"/dist"
	],
	"dependencies": {
		"browserslist": "4.21.9"
	},
	"peerDependencies": {
		"browserslist": "4.21.9"
	},
	"devDependencies": {
		"@terminal-nerds/tsup-config": "workspace:*",
		"@terminal-nerds/typescript-config": "workspace:*",
		"tsup": "7.1.0",
		"typescript": "5.1.6"
	},
	"scripts": {
		"build": "tsup",
		"clean": "concurrently \"pnpm:clean:*\" --group --timings",
		"clean:build": "del \"./dist\"",
		"clean:cache": "del \"./.turbo\" \"./node_modules/.cache\"",
		"dev": "tsup --watch",
		"fix": "concurrently \"pnpm:fix:*\" --group --timings",
		"fix:format": "pretty-quick --branch main --pattern \"packages/browserslist/**\" --verbose",
		"fix:js": "pnpm lint:js --fix",
		"fix:md": "pnpm lint:md --fix",
		"fix:pkg": "syncpack format ; syncpack fix-mismatches",
		"lint": "concurrently \"pnpm:lint:*\" --group --timings",
		"lint:deps": "depcheck",
		"lint:format": "pnpm fix:format --check",
		"lint:js": "DEBUG=\"eslint:cli-engine\" eslint . --cache --cache-location \"./node_modules/.cache/eslint\" --ext .json,.ts ",
		"lint:md": "markdownlint . --config \"../../.markdownlint.json\" --dot --ignore-path \"../../.gitignore\"",
		"lint:types": "tsc --noEmit",
		"prepack": "clean-package --config \"../../.clean-packagerc.cjs\""
	}
}
